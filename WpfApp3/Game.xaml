<Window
        x:Class="WpfApp3.Game"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="         Labirint" 
        Height="800" 
        Width="1000" 
        ResizeMode="NoResize" 
        Icon="Icons/logo_non_bg.ico"
        Style="{StaticResource VS2012WindowStyle}"

        Closing="Window_Closing">
    <Window.Resources>
        <Style x:Key="CanvasStyle1" TargetType="{x:Type Canvas}"/>
    </Window.Resources>
    <Grid Background="#FF252525">
        <Rectangle Height="680" Margin="10,0,0,0" Stroke="#FF15F3CA" VerticalAlignment="Center" RadiusX="12" RadiusY="12" HorizontalAlignment="Left" Width="790"/>
        <StackPanel x:Name="panel1" Height="680" Margin="10,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left" Width="790"/>
    </Grid>
</Window>
