<Window Style="{DynamicResource WindowStyle1}" x:Class="WpfApp3.Game"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        mc:Ignorable="d"
        Title="Game" Height="800" Width="1000" ResizeMode="NoResize" BorderBrush="#FF15F3CA" Background="#FF252525" Foreground="#FF15F3CA" Closing="Window_Closing">
    <Window.Resources>
        <Style x:Key="CanvasStyle1" TargetType="{x:Type Canvas}"/>
    </Window.Resources>
    <Grid Background="#FF252525">
        <Rectangle Height="764" Margin="10,10,200,10" Stroke="#FF15F3CA" VerticalAlignment="Center" RadiusX="12" RadiusY="12"/>
    </Grid>
</Window>
